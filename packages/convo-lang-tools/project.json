{
    "name": "convo-lang-tools",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/convo-lang-tools/src",
    "projectType": "library",
    "targets": {
        "test-ext": {
            "executor": "nx:run-commands",
            "options": {
                "_command": "./packages/convo-lang-tools/lsp/build-lsp.sh && code --extensionDevelopmentPath=$(pwd)/packages/convo-lang-tools/ --disable-extensions ../mindark-knowledge",
                "command": "./packages/convo-lang-tools/lsp/build-lsp.sh && code --extensionDevelopmentPath=$(pwd)/packages/convo-lang-tools/ ../convo-lang-make-example",
                "__command": "./packages/convo-lang-tools/lsp/build-lsp.sh && code --extensionDevelopmentPath=$(pwd)/packages/convo-lang-tools/ ../guardians-collective"
            }
        },
        "publish-ext": {
            "executor": "nx:run-commands",
            "options": {
                "cwd": "packages/convo-lang-tools",
                "command": "./publish.sh"
            }
        },
        "publish-ext-ovsx": {
            "executor": "nx:run-commands",
            "options": {
                "cwd": "packages/convo-lang-tools",
                "command": "./publish-ovsx.sh"
            }
        },
        "mate-build": {
            "executor": "nx:run-commands",
            "options": {
                "cwd": "packages/convo-lang-tools",
                "command": "./mate-build.sh"
            }
        },
        "mate-install": {
            "executor": "nx:run-commands",
            "options": {
                "cwd": "packages/convo-lang-tools",
                "command": "./mate-install.sh"
            }
        }
    },
    "tags": []
}
